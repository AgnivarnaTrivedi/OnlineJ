<% unless flash.empty? %>
<script type="text/javascript">
$(document).ready(function() {
   <% flash.each do |f| %>
   <% type = f[0].gsub('notice', 'info').gsub('alert','warning') %>
    toastr['<%= type %>']('<%= f[1] %>');
   <% end %>
})
</script>
<% end %>
