<% @Submissions.each_with_index do |submission, index|%>
  <%=  submission[:status_code]%>
  <%= @Users[index][:name]%>
  <%= @Problems[index][:name] %>
  <%= @Contests[index] %>
<% end %>

<%= paginate @Submissions , theme: 'twitter-bootstrap-3',
                pagination_class: "pagination pagination-inverse" %>
